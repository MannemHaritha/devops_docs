- hosts: localhost
  become: yes
  gather_facts : false
  tasks :
    - name: download maven
      get_url :
        url : https://downloads.apache.org/maven/binaries/apache-maven-3.0.5-bin.tar.gz
        dest : /opt
        mode: '0755'
    - name: extract maven
      unarchive:
        src: /opt/apache-maven-3.0.5-bin.tar.gz
        dest: /opt
        remote_src: True
    - name: remote after extract
      file:
        path: /opt/apache-maven-3.0.5-bin.tar.gz
        state: absent
		
